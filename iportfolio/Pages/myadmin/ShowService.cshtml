@page
@model iportfolio.Pages.myadmin.ShowServiceModel
@{
}
<div class="container">
<div class="row justify-content-center">
    <div class="col-md-10">
        <h1>Show Service</h1>
    </div>
</div>
<div class="row justify-content-center">
    <div class="col-md-10">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Title</th>
                    <th scope="col">Icon Name</th>
                    <th scope="col" colspan="2" align="center">Action</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.service)
                {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <td>@item.Title</td>
                        <td>@item.IconName</td>
                        

                        <td>
                            <a class="btn btn-primary" asp-page="UpdateService" asp-route-id="@item.Id">Update</a>
                        </td>
                        <td>
                            <a class="btn btn-danger " asp-page-handler="Delete" asp-route-id="@item.Id">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>
</div>
