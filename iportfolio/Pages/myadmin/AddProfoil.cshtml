@page
@model iportfolio.Pages.myadmin.AddProfoilModel
@{
}
<div class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="row justify-content-center">
            <div class="col-md-4">
                <h1>
                    Add Profoil
                </h1>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Name : </label>
                <input class="form-control" asp-for="profoil.Name">
                <span asp-validation-for="profoil.Name" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Birthday :</label>
                <input class="form-control" asp-for="profoil.Birthday">
                <span asp-validation-for="profoil.Birthday" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Age : </label>
                <input class="form-control" asp-for="profoil.Age">
                <span asp-validation-for="profoil.Age" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Website : </label>
                <input class="form-control" asp-for="profoil.Website">
                <span asp-validation-for="profoil.Website" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Degree : </label>
                <input class="form-control" asp-for="profoil.Degree">
                <span asp-validation-for="profoil.Degree" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Phone : </label>
                <input class="form-control" asp-for="profoil.Phone">
                <span asp-validation-for="profoil.Phone" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Email : </label>
                <input class="form-control" asp-for="profoil.Email">
                <span asp-validation-for="profoil.Email" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">City : </label>
                <input class="form-control" asp-for="profoil.City">
                <span asp-validation-for="profoil.City" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">FreeLanceStatus : </label>
                <input class="form-control" asp-for="profoil.FreelanceStatus">
                <span asp-validation-for="profoil.FreelanceStatus" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Description one : </label>
                <input class="form-control" asp-for="profoil.DescOne">
                <span asp-validation-for="profoil.DescOne" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Description Two : </label>
                <input class="form-control" asp-for="profoil.DescTwo">
                <span asp-validation-for="profoil.DescTwo" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <label class="form-label">Image : </label>
                <input class="form-control" asp-for="profoil.Photo">
                <span asp-validation-for="profoil.Photo" class="text-danger"></span>
            </div>
        </div>
        <div class="row justify-content-center">
            <div class="col-md-4">
                <input type="submit" value="Add Profoil" class="btn btn-primary">
            </div>
        </div>
    </form>
</div>
